    order_id INTEGER,
    order_date DATE,
    amount REAL,
    product_id INTEGER
);
</sql><sql name="SQL 2*">INSERT INTO online_sales VALUES
(1, '2024-01-10', 500, 101),
(2, '2024-01-15', 1200, 102),
(3, '2024-02-05', 800, 103),
(4, '2024-02-20', 1500, 101),
(5, '2024-03-12', 2000, 104),
(6, '2024-03-18', 700, 102),
(7, '2024-03-25', 950, 105);
</sql><sql name="SQL 3*">SELECT 
    strftime('%Y', order_date) AS year,
    strftime('%m', order_date) AS month,
    SUM(amount) AS total_revenue,
    COUNT(DISTINCT order_id) AS order_volume
FROM online_sales
GROUP BY year, month
ORDER BY year, month;
</sql><sql name="SQL 4*">SELECT 
    strftime('%m', order_date) AS month,
    SUM(amount) AS total_revenue
FROM online_sales
GROUP BY month
ORDER BY total_revenue DESC
LIMIT 3;
</sql><current_tab id="3"/></tab_sql></sqlb_project>
